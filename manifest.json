{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "0.0.5",
  "description": "Edit your last message on WA Web app by pressing the Up Arrow key.",
  "author": "valentincostadev@gmail.com",
  "default_locale": "en",
  "browser_specific_settings": {
    "gecko": {
      "id": "wa-edit-last@valentincostadev.com",
      "strict_min_version": "109.0"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}